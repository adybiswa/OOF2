OOF.Windows.Graphics.New()
OOF.Graphics_1.Settings.New_Layer_Policy(policy='Single')
OOF.Microstructure.Create_From_ImageFile(filename='examples/el_shape.png', microstructure_name='el_shape.png', height=automatic, width=automatic)
OOF.Image.AutoGroup(image='el_shape.png:el_shape.png', name_template='%c')
OOF.PixelGroup.Rename(microstructure='el_shape.png', group='#00ff00', new_name='green')
OOF.Material.New(name='green-material', material_type='bulk')
OOF.Property.Copy(property='Mechanical:Elasticity:Isotropic', new_name='green_elasticity')
OOF.Property.Parametrize.Mechanical.Elasticity.Isotropic.green_elasticity(cijkl=IsotropicRank4TensorEnu(young=1.0,poisson=0.3))
OOF.Material.Add_property(name='green-material', property='Mechanical:Elasticity:Isotropic:green_elasticity')
OOF.Material.Assign(material='green-material', microstructure='el_shape.png', pixels='green')
OOF.Skeleton.New(name='skeleton', microstructure='el_shape.png', x_elements=20, y_elements=20, skeleton_geometry=QuadSkeleton(left_right_periodicity=False,top_bottom_periodicity=False))
OOF.Graphics_1.Toolbox.Select_Segment.Rectangle(skeleton='el_shape.png:skeleton', points=[Point(3.09,9.76), Point(10.475,10.215)], shift=False, ctrl=False)
OOF.Graphics_1.Toolbox.Select_Segment.Single_Segment(skeleton='el_shape.png:skeleton', points=[Point(3.125,10.005)], shift=True, ctrl=False)
OOF.Skeleton.Boundary.Modify(skeleton='el_shape.png:skeleton', boundary='top', modifier=RemoveSegments(group=selection))
OOF.Graphics_1.Settings.List_All_Layers(1)
OOF.Graphics_1.Layer.Select(n=6)
OOF.Graphics_1.Layer.Edit(n=6, category='Skeleton', what='<topmost>', how=SelectedSkeletonBoundaryDisplay(color=RGBAColor(red=1.0,green=0.5,blue=0.0,alpha=1.0),linewidth=4,dotsize=4,arrowsize=5))
OOF.Graphics_1.Toolbox.Select_Segment.Rectangle(skeleton='el_shape.png:skeleton', points=[Point(9.703125,2.83), Point(10.193125,10.215)], shift=False, ctrl=False)
OOF.Skeleton.Boundary.Modify(skeleton='el_shape.png:skeleton', boundary='right', modifier=RemoveSegments(group=selection))
OOF.Mesh.New(name='mesh', skeleton='el_shape.png:skeleton', element_types=['D3_3', 'T6_6', 'Q8_8'])
OOF.Graphics_1.Settings.List_All_Layers(0)
OOF.Graphics_1.Layer.Hide(n=4)
OOF.Graphics_1.Layer.Select(n=4)
OOF.Subproblem.Field.Define(subproblem='el_shape.png:skeleton:mesh:default', field=Displacement)
OOF.Subproblem.Field.Activate(subproblem='el_shape.png:skeleton:mesh:default', field=Displacement)
OOF.Mesh.Field.In_Plane(mesh='el_shape.png:skeleton:mesh', field=Displacement)
OOF.Subproblem.Equation.Activate(subproblem='el_shape.png:skeleton:mesh:default', equation=Force_Balance)
OOF.Mesh.Boundary_Conditions.New(name='bc', mesh='el_shape.png:skeleton:mesh', condition=DirichletBC(field=Displacement,field_component='x',equation=Force_Balance,eqn_component='x',profile=ConstantProfile(value=0.0),boundary='left'))
OOF.Mesh.Boundary_Conditions.New(name='bc<2>', mesh='el_shape.png:skeleton:mesh', condition=DirichletBC(field=Displacement,field_component='y',equation=Force_Balance,eqn_component='y',profile=ConstantProfile(value=0.0),boundary='left'))
OOF.Mesh.Boundary_Conditions.New(name='bc<3>', mesh='el_shape.png:skeleton:mesh', condition=DirichletBC(field=Displacement,field_component='x',equation=Force_Balance,eqn_component='x',profile=ConstantProfile(value=0.0),boundary='top'))
OOF.Mesh.Boundary_Conditions.New(name='bc<4>', mesh='el_shape.png:skeleton:mesh', condition=DirichletBC(field=Displacement,field_component='y',equation=Force_Balance,eqn_component='y',profile=ConstantProfile(value=0.0),boundary='top'))
OOF.Mesh.Boundary_Conditions.New(name='bc<5>', mesh='el_shape.png:skeleton:mesh', condition=DirichletBC(field=Displacement,field_component='y',equation=Force_Balance,eqn_component='y',profile=ConstantProfile(value=-2),boundary='right'))
OOF.Subproblem.Set_Solver(subproblem='el_shape.png:skeleton:mesh:default', solver_mode=BasicSolverMode(time_stepper=BasicStaticDriver(),matrix_method=BasicIterative(tolerance=1e-13,max_iterations=1000)))
OOF.Mesh.Solve(mesh='el_shape.png:skeleton:mesh', endtime=0.0)
