OOF.Microstructure.Create_From_ImageFile(filename='examples/small.ppm', microstructure_name='small.ppm', height=1.5, width=1.0)
OOF.Image.AutoGroup(image='small.ppm:small.ppm', name_template='%c')
OOF.Settings.Graphics_Defaults.New_Layer_Policy(policy='Single')
OOF.Windows.Graphics.New()
OOF.Graphics_1.Layer.Select(n=0)
OOF.Skeleton.New(name='skeleton', microstructure='small.ppm', x_elements=4, y_elements=6, skeleton_geometry=QuadSkeleton(left_right_periodicity=False,top_bottom_periodicity=False))
OOF.Skeleton.Modify(skeleton='small.ppm:skeleton', modifier=Refine(targets=CheckHomogeneity(threshold=1),criterion=Unconditionally(),degree=Trisection(rule_set='liberal'),alpha=0.75))
OOF.Skeleton.Undo(skeleton='small.ppm:skeleton')
OOF.Skeleton.Redo(skeleton='small.ppm:skeleton')
OOF.Skeleton.Modify(skeleton='small.ppm:skeleton', modifier=SnapNodes(targets=SnapAll(),criterion=AverageEnergy(alpha=0.75)))
OOF.Skeleton.Rename(skeleton='small.ppm:skeleton', name='bones')
OOF.Skeleton.Copy(skeleton='small.ppm:bones', name='skeleton')
OOF.Skeleton.Undo(skeleton='small.ppm:bones')
OOF.Skeleton.Delete(skeleton='small.ppm:skeleton')
OOF.File.Save.Skeleton(filename='bones.dat', mode='w', format='ascii', skeleton='small.ppm:bones')
OOF.Skeleton.Delete(skeleton='small.ppm:bones')
OOF.Skeleton.New(name='skeleton', microstructure='small.ppm', x_elements=4, y_elements=6, skeleton_geometry=QuadSkeleton(left_right_periodicity=False,top_bottom_periodicity=False))
OOF.Microstructure.Delete(microstructure='small.ppm')
