OOF.Microstructure.Create_From_ImageFile(filename='examples/small.ppm', microstructure_name='small.ppm', height=automatic, width=automatic)
OOF.PixelGroup.New(name='green', microstructure='small.ppm')
OOF.Settings.Graphics_Defaults.New_Layer_Policy(policy='Single')
OOF.Windows.Graphics.New()
OOF.Graphics_1.Layer.Select(n=0)
OOF.Graphics_1.Toolbox.Pixel_Select.Color(source='small.ppm:small.ppm', range=DeltaRGB(delta_red=0,delta_green=0,delta_blue=0), points=[Point(40.0875,75.525)], shift=False, ctrl=False)
OOF.PixelGroup.AddSelection(microstructure='small.ppm', group='green')
OOF.Graphics_1.File.Close()
OOF.Material.New(name='test', material_type='bulk')
OOF.Property.Copy(property='Color', new_name='green')
OOF.Property.Parametrize.Color.green(color=TranslucentGray(value=0.513889,alpha=0.833333))
OOF.Material.Add_property(name='test', property='Color:green')
OOF.Property.Parametrize.Thermal.Conductivity.Anisotropic.Cubic(kappa=CubicRank2Tensor(xx=123))
OOF.Material.Add_property(name='test', property='Thermal:Conductivity:Anisotropic:Cubic')
OOF.Material.Copy(name='test', new_name='copy')
OOF.Material.Remove_property(name='copy', property='Thermal:Conductivity:Anisotropic:Cubic')
OOF.Property.Delete(property='Color:green')
OOF.Material.Rename(material='test', name='material test')
OOF.Material.New(name='another test', material_type='bulk')
OOF.Property.Parametrize.Thermal.Conductivity.Anisotropic.Cubic(kappa=CubicRank2Tensor(xx=123.0))
OOF.Property.Copy(property='Thermal:Conductivity:Anisotropic:Cubic', new_name='cubic_copy')
OOF.Material.Add_property(name='another test', property='Thermal:Conductivity:Anisotropic:Cubic:cubic_copy')
OOF.Material.Assign(material='another test', microstructure='small.ppm', pixels='green')
OOF.Windows.Graphics.New()
OOF.Graphics_1.Layer.Select(n=0)
OOF.Graphics_1.Toolbox.Pixel_Info.Query(x=51, y=82)
OOF.Material.Remove(microstructure='small.ppm', pixels='green')
OOF.Graphics_1.Toolbox.Pixel_Info.Query(x=31, y=56)
OOF.Graphics_1.Toolbox.Pixel_Info.Query(x=87, y=47)
OOF.Material.Assign(material='another test', microstructure='small.ppm', pixels=every)
OOF.Graphics_1.Toolbox.Pixel_Info.Query(x=46, y=79)
OOF.Graphics_1.Toolbox.Pixel_Info.Query(x=135, y=9)
OOF.Graphics_1.Toolbox.Pixel_Info.Query(x=126, y=60)
OOF.Material.Remove(microstructure='small.ppm', pixels='green')
OOF.Graphics_1.Toolbox.Pixel_Info.Query(x=54, y=64)
OOF.Graphics_1.Toolbox.Pixel_Info.Query(x=109, y=119)
OOF.File.Save.Materials(filename='material.dat', mode='w', format='ascii', materials=['another test'])
OOF.Microstructure.Delete(microstructure='small.ppm')
OOF.Material.Delete(name='another test')
OOF.Material.Delete(name='copy')
OOF.Material.Delete(name='material test')
